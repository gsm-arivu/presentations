/*-------------pyramid2 bullets (bottom-up pyramid)-----------------*/
/*-------------pyramid2 bullets (bottom-up pyramid, centered)-----------------*/

section.pyramid2 ul {
  display: flex;
  flex-direction: column-reverse;  /* Reverse visual stacking: bottom to top */
  align-items: center;             /* ✅ Horizontally center all bullet shapes */
  gap: 1px;
  list-style: none;
  padding: 0;
  margin: 0;
}

/* Base style for top-level bullets */
section.pyramid2 > ul > li {
  position: relative;
  text-align: center;  
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 0.8rem;  
  color:white;
    
}

/* Emoticon style */
section.pyramid2 > ul > li::before {
  content: ' ';
  
}

/* Visual layers from bottom to top */

/* 1st (bottom layer - largest trapezoid) */
section.pyramid2 > ul > li:nth-child(1) {
  clip-path: polygon(8.5% 0%, 91.5% 0%, 100% 100%, 0% 100%);
  width: 82%;
  background-color: #6b6403;
}

/* 2nd layer */
section.pyramid2 > ul > li:nth-child(2) {
  clip-path: polygon(11% 0%, 89% 0%, 100% 100%, 0% 100%);
  width: 67%;
  background-color: #790404;
}

/* 3rd layer */
section.pyramid2 > ul > li:nth-child(3) {
  clip-path: polygon(15.5% 0%, 84.5% 0%, 100% 100%, 0% 100%);
  width: 51%;
  background-color: #178504;
}

/* 4th (top layer - triangle) */
