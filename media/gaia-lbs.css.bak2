/* panel_transparent: responsive and evenly-spaced flex layout */

/* Panel container layout */
section.panel_transparent ul {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-evenly; /* ensures even spacing of fewer items */
  gap: 24px;
  padding-left: 0;
  list-style: none;
  margin: 0;
