blockquote {
  margin: 0;
  padding: 0 1em;
  position: relative;
  font-family: Georgia, 'Times New Roman', Times, serif;
  font-style: italic;
  

  &::after,
  &::before {
    content: '“';
    display: block;
    font-family: 'Times New Roman', serif;
    font-weight: bold;
    position: absolute;
    font-size: 3rem;
    color: black;
  }

  &::before {
    top: -40px;
    left: -40px;
  }

  &::after {
    right: -40px;
    bottom: -40px;
    transform: rotate(180deg);
  }

  > *:first-child {
    margin-top: 0;
  }
}

