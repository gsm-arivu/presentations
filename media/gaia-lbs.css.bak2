/*--------------------------------------------*/

/*-------------pyramid bullets-----------------*/
/*-------------pyramid bullets perfectly aligned-----------------*/

section.pyramid ul {
  display: grid;
  grid-template-rows: repeat(4, auto);
  gap: 0.03rem; /* gap between shapes */
  justify-items: center;
  list-style: none;
  padding: 0;
  margin: 0;
}

/* Base style: only for top-level bullets */
section.pyramid > ul > li {
  position: relative;
  text-align: center;  
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 0.75rem;  
  border: 1px solid black;
  color: white;
 }

/* Emoticon style */
section.pyramid > ul > li::before {
  content: attr(data-icon);
  font-size: 1rem;
  display: block;  
  color: white;
}

/* Geometric alignment of each shape */
section.pyramid > ul > li:nth-child(1) {
  clip-path: polygon(50% 0%, 100% 100%, 0% 100%);
  width: 34%;
  height: 6rem;
  background-color: #7c4cdb;
}

section.pyramid > ul > li:nth-child(2) {
  clip-path: polygon(15% 0%, 85% 0%, 100% 100%, 0% 100%);
  width: 51%;
  background-color: #178504;
}

section.pyramid > ul > li:nth-child(3) {
  clip-path: polygon(11% 0%, 89% 0%, 100% 100%, 0% 100%);
  width: 67%;
  background-color: #790404;
}

section.pyramid > ul > li:nth-child(4) {
  clip-path: polygon(8.5% 0%, 91.5% 0%, 100% 100%, 0% 100%);
  width: 82%;
  background-color: #6b6403;
}

/* Sub-bullet flattening */
section.pyramid ul li ul,
section.pyramid ul li ul li {
  all: unset;
  display: block;
  color: white;
  font-size: 0.6rem;
  font-weight: normal;
  text-align: center; 
  line-height: 0.6rem;
}

