/*  hexagon bullets */
/* Hexagonal bullets perfectly centered, starting left at -180deg */
section.hexagon {
  position: relative;
  width: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
  z-index: 0;
}

section.hexagon ul {
  list-style: none;
  margin: 0;
  padding: 0;
  position: absolute;
  top: 40%; /* Exactly vertical center */
  left: 40%; /* Exactly horizontal center */
  width: 0;
  height: 0;
  transform: translate(-50%, -50%);
}

section.hexagon li {
  position: absolute;
  width: 200px;
  height: 200px;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-weight: normal;
  font-size: 24px;
  text-align: center;
  box-shadow: 2px 4px 10px rgba(0, 0, 0, 0.2);
  transition: transform 0.2s ease, box-shadow 0.2s ease;
}

section.hexagon li:hover {
  transform: scale(1.1);
  box-shadow: 4px 6px 20px rgba(0, 0, 0, 0.3);
}

/* Circular positioning precisely centered (distance reduced to 160px) */
section.hexagon li:nth-child(1) { transform: rotate(-180deg) translate(200px) rotate(180deg); }
section.hexagon li:nth-child(2) { transform: rotate(-120deg) translate(200px) rotate(120deg); }
section.hexagon li:nth-child(3) { transform: rotate(-60deg) translate(200px) rotate(60deg); }
section.hexagon li:nth-child(4) { transform: rotate(0deg) translate(200px) rotate(0deg); }
section.hexagon li:nth-child(5) { transform: rotate(60deg) translate(200px) rotate(-60deg); }
section.hexagon li:nth-child(6) { transform: rotate(120deg) translate(200px) rotate(-120deg); }

/* Unique colors (unchanged) */
section.hexagon li:nth-child(1) { background-color: #34495E; } /* Midnight Blue Gray */
section.hexagon li:nth-child(2) { background-color: #16A085; } /* Teal */
section.hexagon li:nth-child(3) { background-color: #2980B9; } /* Calm Blue */
section.hexagon li:nth-child(4) { background-color: #8E44AD; } /* Muted Purple */
section.hexagon li:nth-child(5) { background-color: #D35400; } /* Warm Orange */
section.hexagon li:nth-child(6) { background-color: #1ABC9C; } /* Aqua Green */

/* Precise centering of h1 above hexagon bullets */
section.hexagon h1 {
  position: absolute;
  top: 8%;          /* Adjust vertically as needed */
  left: 25%;         /* Exact horizontal center */
  transform: translateX(-50%);
  width: 100%;
  text-align: center; /* Ensures horizontal text centering */
  margin: 0;          /* Removes any default margin */
  padding: -10;         /* Removes padding */
  z-index: 0;         /* Above bullets */
  line-height: 1 !important;  
}


