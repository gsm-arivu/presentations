/*----------------------------------------*/

/*--------------shining animation--------------------*/
section.shine ul > li {
  position: relative;
  
  background: inherit;
  z-index: 1;
}

section.shine ul > li::after {
  content: "";
  position: absolute;
  top: 0;
  left: -150%;
  height: 100%;
  width: 60%;
  background: linear-gradient(
    120deg,
    rgba(255, 255, 255, 0.1) 0%,
    rgba(255, 255, 255, 0.6) 50%,
    rgba(255, 255, 255, 0.1) 100%
  );
  mix-blend-mode: overlay;
  transform: skewX(-25deg);
  animation: shine-sweep 5s infinite;
  pointer-events: none;
  z-index: 2;
}

@keyframes shine-sweep {
  0% {
    left: -150%;
  }
  100% {
    left: 150%;
  }
}

/*---------------------------------------*/

/*--------------crawl animation--------------------*/

section.crawl li {
  position: relative;
  padding: 16px 20px;
  margin-bottom: 14px;
  background-color: #F8F9FA;
  color: #2C3E50;
  font-size: 1.05em;
  font-weight: 500;
  border-radius: 15px;
  z-index: 0;
  overflow: hidden;
}

/* Add crawling effect via SVG border using ::after */
section.crawl li::after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 15px;
  box-sizing: border-box;
  z-index: 0;
  pointer-events: none;

  background-image: 
    linear-gradient(to right, yellow 0% 50%, black 0% 50%);
  background-size: 5px 2px;
  background-repeat: repeat;
  background-position: 0 100%;

  mask: 
    linear-gradient(#000 0 0) content-box, 
    linear-gradient(#000 0 0);
  mask-composite: exclude;
  -webkit-mask-composite: destination-out;

  animation: border-crawl 1s linear infinite;
  border: 2px dashed transparent;
}

/* Animate background position to create crawl effect */
@keyframes border-crawl {
  0% { background-position: 0 100%; }
  100% { background-position: 10px 100%; }
}


/*------------------------------------------------------*/

/*-------------------marchants animation--------------------------*/

section.marchants li {
  position: relative;
  padding: 16px 20px;
  margin-bottom: 14px;
  background-color: #F8F9FA;
  color: #2C3E50;
  font-size: 1.05em;
  font-weight: 500;
  border-radius: 15px;
  z-index: 0;
  overflow: hidden;
}

/* Add marchantsing effect via SVG border using ::after */
section.marchants li::after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 15px;
  box-sizing: border-box;
  z-index: 1;
  pointer-events: none;

  background-image: 
    linear-gradient(to right, #7734db 0% 50%, transparent 100% 100%);
  background-size: 10px 20px;
  background-repeat: repeat;
  background-position: 0 100%;

  mask: 
    linear-gradient(#000 0 0) content-box, 
    linear-gradient(#000 0 0);
  mask-composite: exclude;
  -webkit-mask-composite: destination-out;

  animation: border-marchants 0.5s linear infinite;
  border: 2px solid transparent;
}

/* Animate background position to create marchants effect */
@keyframes border-marchants {
  0% { background-position: 0 100%; }
  100% { background-position: 10px 100%; }
}
/*---------------bubble class---------------*/

/*========================================*/
/* BUBBLE ANIMATION FOR MARP BULLET BOXES */
/*========================================*/
section.bubble ul > li {
  position: relative;
  overflow: hidden;
  z-index: 1;
  isolation: isolate;
}

section.bubble ul > li::before,
section.bubble ul > li::after {
  content: "";
  position: absolute;
  top: 0;
  left: -150px;
  width: 400%;
  height: 100%;
  pointer-events: none;
  z-index: 0;
  background-repeat: no-repeat;
  background-size:
    80px 20px,
    120px 25px,
    60px 15px,
    100px 22px;
  background-position:
    10% 20%,
    50% 60%,
    70% 30%,
    90% 70%;
  background-image:
    radial-gradient(ellipse 40px 10px at center, transparent 40%, rgba(255, 105, 180, 0.5) 41%, transparent 45%),
    radial-gradient(ellipse 60px 12px at center, transparent 40%, rgba(255, 105, 180, 0.5) 41%, transparent 45%),
    radial-gradient(ellipse 30px 7px at center, transparent 40%, rgba(255, 105, 180, 0.4) 41%, transparent 45%),
    radial-gradient(ellipse 50px 11px at center, transparent 40%, rgba(255, 105, 180, 0.4) 41%, transparent 45%);
  animation: bubble-slide 30s linear infinite;
}

section.bubble ul > li::after {
  animation-delay: 10s;
  opacity: 0.7;
}

@keyframes bubble-slide {
  0% {
    left: -150px;
  }
  100% {
    left: 100%;
  }
}


/*-----------------border glow--------------------*/
section.borderglow ul > li {
  position: relative;
  z-index: 0;
  border-radius: inherit;
}

/* Glowing sweep animation overlay */
section.borderglow ul > li::after {
  content: "";
  position: absolute;
  top: -2px;
  left: -2px;
  right: -2px;
  bottom: -2px;
  border-radius: inherit;
  pointer-events: none;
  z-index: 1;
  background: linear-gradient(
    90deg,
    rgba(255, 255, 255, 0) 0%,
    rgba(255, 255, 255, 0.5) 50%,
    rgba(255, 255, 255, 0) 100%
  );
  background-size: 200% 100%;
  background-repeat: no-repeat;
  animation: borderglow-sweep 2s linear infinite;
  box-shadow: 0 0 8px rgba(255, 255, 255, 0.5);
}
@keyframes borderglow-sweep {
  0% {
    background-position: -100% 0%;
  }
  100% {
    background-position: 200% 0%;
  }
}

/*------------------slidein---------------------*/
/* 1. Slide-in for FRAGMENTED bullets (appear on click) */
section.slidein ul > li.fragment {
  opacity: 0;
  transform: translateX(-40px);
}

section.slidein ul > li.fragment.visible {
  animation: slideInLeft 0.6s ease-out forwards;
}

/* 2. Slide-in for UNFRAGMENTED bullets (appear all at once, but with stagger) */
section.slidein ul > li:not(.fragment) {
  opacity: 0;
  transform: translateX(-40px);
  animation: slideInLeft 0.6s ease-out forwards;
}

section.slidein ul > li:not(.fragment):nth-child(1) { animation-delay: 0.1s; }
section.slidein ul > li:not(.fragment):nth-child(2) { animation-delay: 0.3s; }
section.slidein ul > li:not(.fragment):nth-child(3) { animation-delay: 0.5s; }
section.slidein ul > li:not(.fragment):nth-child(4) { animation-delay: 0.7s; }
section.slidein ul > li:not(.fragment):nth-child(5) { animation-delay: 0.9s; }

/* Keyframes for both */
@keyframes slideInLeft {
  from {
    opacity: 0;
    transform: translateX(-40px);
  }
  to {
    opacity: 1;
    transform: translateX(0);
  }
}


/*------highlighting and redboxing-----------------------*/
.highlight {
  color: black;
  font-weight: bold;
  background: yellow;  
}
.redbox {
  border: 6px dotted red;
  border-radius: 10px;
  
}

/*-------------------color changing background ---------------------*/

/* SPACE SLIDE BACKGROUND - LIKE A VIDEO SCREEN */
/* Apply animated background color transitions */
section.backcolor {
  position: relative;
  animation: spaceFade 40s infinite alternate;
  color: white;
  overflow: hidden;
}

/* Ensure foreground content appears above */
section.backcolor > * {
  position: relative;
  z-index: 1;
}

/* Smooth cycling between deep dark colors */
@keyframes spaceFade {
  0%   { background-color: #066a83; }  /* Deep navy */
  16%  { background-color: #0b0b81; }  /* Indigo night */
  32%  { background-color: #512424; }  /* Charcoal black */
  48%  { background-color: #056912; }  /* Grey-blue dusk */
  64%  { background-color: #8e0509; }  /* Midnight blue */
  80%  { background-color: #d57a30; }  /* Cosmic slate */
  100% { background-color: #9da1a1; }  /* Back to start */
}

/*----------------screen video-------------------------*/

/* Container for the background video */
section.screen {
  position: relative;
  overflow: hidden;
  color: white;
}

/* Embed full-screen looping video */
section.screen::before {
  content: "";
  position: absolute;
  top: 0; left: 0;
  width: 100%;
  height: 100%;
  z-index: 0;
  background: black; /* Fallback */
  pointer-events: none;
}

/* Video element styling */
section.screen video.bg-video {
  position: absolute;
  top: 0; left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
  z-index: 0;
  pointer-events: none;
}

/* Ensure content appears on top */
section.screen > * {
  position: relative;
  z-index: 1;
}





