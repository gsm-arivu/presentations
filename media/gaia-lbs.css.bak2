/* @theme gaia-lbs */


@import 'gaia';
@import 'https://cdn.bootcdn.net/ajax/libs/font-awesome/6.4.0/css/all.min.css';
@import url(https://db.onlinewebfonts.com/c/f1cfb0d208038e21ad90267089ab2584?family=Felix+Titling+V2);

/* Common header base */
.lbsnaa-header {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 70px;
  padding: 10px 25px;
  display: flex;  
  align-items: center;
  box-sizing: border-box;
 
}

/* Logo styling (normal) */
.lbsnaa-header img {
  height: 60px;
  
}

/* Logo inversion for invert class */
section.invert .lbsnaa-header img {
  filter: invert(1);
}

/* Logo inversion for invert class */
section.gaia .lbsnaa-header img {
  filter: invert(1);
}

/* Title styling */
.lbsnaa-header h1 {
  margin: 0;
  font-size: 24px;
  font-weight: normal;
  color: var(--color-foreground);
  line-height: 0;
  padding: 10px;
}

/* Title styling */
.lbsnaa-header p {
  margin: 0;
  font-size: 24px;
  font-weight: normal;
  color: var(--color-foreground);
  line-height: 0;
  padding: 10px;
}

/* Pagination styling */
section::after {
  content: attr(data-marpit-pagination) " / " attr(data-marpit-pagination-total);
  position: absolute;
  bottom: -10px;
  right: -10px;
  font-size: 0.6em;
  color: var(--color-foreground);
  font-family: sans-serif;
}


/* bullet styles begin here */


/* box bullets */
/* Global definition of the .box class */
section.box ul {
    list-style: none;
    padding: 0;
  }
  section.box li {
    padding: 14px;
    border-radius: 12px;
    color: white;
    font-weight: normal;
    margin-bottom: 14px;
    font-size: 1.05em;
    box-shadow: 3px 5px 15px rgba(0, 0, 0, 0.25);
    transition: transform 0.2s ease;
  }
  section.box li:hover {
    transform: scale(1.02);
  }
  /* Unique background colors for each bullet */
  section.box li:nth-child(1) { background-color: #FF6F61; }  /* Coral Red */
  section.box li:nth-child(2) { background-color: #6A5ACD; }  /* Slate Blue */
  section.box li:nth-child(3) { background-color: #26A69A; }  /* Teal Green */
  section.box li:nth-child(4) { background-color: #FFA500; }  /* Orange */
  section.box li:nth-child(5) { background-color: #9C27B0; }  /* Violet */
  section.box li:nth-child(6) { background-color: #42A5F5; }  /* Sky Blue */


/* capsule bullets */
  /* Global definition of the .capsule class */
section.capsule ul {
  list-style: none;
  padding: 0;
}
section.capsule li {
  padding: 14px 24px;
  border-radius: 999px; /* pill shape */
  color: white;
  font-weight: 500;
  margin-bottom: 14px;
  font-size: 1.05em;
  box-shadow: 3px 5px 15px rgba(0, 0, 0, 0.25);
  transition: transform 0.2s ease;
}

section.capsule li::before {
  content: ''; /* Replace with relevant icon or emoji */
  
}

section.capsule li:hover {
  transform: scale(1.02);
}

/* Unique background colors for each bullet (elegant theme) */
section.capsule li:nth-child(1) { background-color: #2E86AB; }  /* Deep Cerulean Blue */
section.capsule li:nth-child(2) { background-color: #6C5B7B; }  /* Muted Plum Purple */
section.capsule li:nth-child(3) { background-color: #3C8D0D; }  /* Olive Green */
section.capsule li:nth-child(4) { background-color: #D4A373; }  /* Muted Sandstone */
section.capsule li:nth-child(5) { background-color: #4E4E50; }  /* Charcoal Gray */
section.capsule li:nth-child(6) { background-color: #227C9D; }  /* Steel Blue */



/* white bullets */
/* Global definition of the .white class - Formal, Professional Look */
section.white ul {
  list-style: none;
  padding: 0;
  margin: 0;
}

section.white li {
  padding: 16px 20px;  
  background-color: #F8F9FA;       /* Very light gray background */
  color: #2C3E50;                  /* Text in dark slate */
  font-weight: 500;
  margin-bottom: 12px;
  font-size: 1.05em;
  border-radius: 15px;
  box-shadow: 3px 5px 15px rgba(0, 0, 0, 0.25);
  transition: transform 0.2s ease;
  
}

/* Alternate shading for clarity */
section.white li:nth-child(even) {
  background-color: #ECEFF1;       /* Slightly darker light gray */
}

/* Optional bullet-like dot before each list item */
section.white li::before {
  content: "";
  color: #2C3E50;
  margin-right: 12px;
  font-size: 1.1em;
  vertical-align: middle;
}

section.white li:hover {
  transform: scale(1.02);
}


/* white_circle bullets */
/* Global definition of the .white_circle class - Formal, Professional Look */
section.white_circle ul {
  list-style: none;
  padding: 0;
  margin: 0;
}

section.white_circle li {
  padding: 16px 20px;
  border-left: 6px solid #2C3E50;  /* Formal deep blue-gray line */
  background-color: #F8F9FA;       /* Very light gray background */
  color: #2C3E50;                  /* Text in dark slate */
  font-weight: 500;
  margin-bottom: 12px;
  font-size: 1.05em;
  border-radius: 6px;
  box-shadow: 3px 5px 15px rgba(0, 0, 0, 0.25);
  transition: transform 0.2s ease;
}

/* Alternate shading for clarity */
section.white_circle li:nth-child(even) {
  background-color: #ECEFF1;       /* Slightly darker light gray */
}

/* Optional bullet-like dot before each list item */
section.white_circle li::before {
  content: "●";
  color: #2C3E50;
  margin-right: 12px;
  font-size: 1.1em;
  vertical-align: middle;
}

section.white_circle li:hover {
  transform: scale(1.02);
}

/* white_delta bullets */
/* Global definition of the .white_delta class - Academic Style */
section.white_delta ul {
  list-style: none;
  padding: 0;
  margin: 0;
  
}

section.white_delta li {
  padding: 16px 22px;
  background-color: #FDFDFD;        /* Clean white with slight tone */
  color: #1A1A1A;                   /* Near black for better contrast */
  border-left: 5px solid #6A1B9A;   /* Deep academic purple line */
  margin-bottom: 14px;
  font-size: 1.1em;
  font-weight: 400;
  line-height: 1.6;
  border-radius: 4px;
  box-shadow: 3px 5px 15px rgba(0, 0, 0, 0.25);
  transition: transform 0.2s ease;
}

/* Alternate subtle highlights for engagement */
section.white_delta li:nth-child(odd) {
  background-color: #FAF7FF;        /* Light lavender */
}
section.white_delta li:nth-child(even) {
  background-color: #F3F6FB;        /* Soft blue-gray */
}

/* Optional marker before each point */
section.white_delta li::before {
  content: "▶";
  color: #6A1B9A;  /* Same purple as the border */
  margin-right: 10px;
  font-size: 0.9em;
  vertical-align: middle;
}

section.white_delta li:hover {
  transform: scale(1.02);
}


/* card bullets */
/* card as Academic Cards in 3x2 Grid */
section.card ul {
  display: grid;
  grid-template-columns: repeat(3, 1fr);  /* 3 cards per row */
  gap: 20px;
  list-style: none;
  padding: 0;
  margin: 0;
  
}

section.card li {
  background-color: #e35757b8;
  color: #fbf9f9;
  padding: 18px 24px; 
  border-top: 6px solid #6a0118;
  border-radius: 8px;  
  box-shadow: 3px 5px 15px rgba(0, 0, 0, 0.25);
  font-size: 1.05em;
  font-weight: normal;
  line-height: 1.5;
  align-items: flex-start;
  transition: transform 0.2s ease;
}

/* Optional hover effect */
section.card li:hover {
  transform: translateY(-4px);
}


/* Optional responsiveness for small screens */
@media (max-width: 768px) {
  section.card ul {
    grid-template-columns: repeat(2, 1fr); /* 2 cards per row */
  }
}
@media (max-width: 480px) {
  section.card ul {
    grid-template-columns: 1fr; /* 1 card per row */
  }
}


/* panel bullets */
/* Panel container layout */
section.panel ul {
  display: flex;
  flex-wrap: wrap;
  gap: 16px;
  padding-left: 0;
  list-style: none;
  margin: 0;
}

/* Panel box */
section.panel > ul > li {
  flex: 1 1 23%;
  display: flex;
  flex-direction: column;
  border-radius: 10px;
  overflow: hidden;
  box-shadow: 2px 4px 12px rgba(0, 0, 0, 0.2);
  min-width: 220px;
  min-height: 400px;
  background-color: #f9f9f9;
  margin: 0;
  padding: 0;
  transition: transform 0.3s ease, box-shadow 0.3s ease;
}

/* Hover effect */
section.panel > ul > li:hover {
  transform: translateY(-6px);
  box-shadow: 4px 6px 16px rgba(0, 0, 0, 0.3);
}

/* Remove bullet marker */
section.panel li::marker {
  display: none;
}

/* Panel heading */
section.panel > ul > li > p:first-child {
  background-color: #333;
  color: #fff;
  font-weight: normal;
  text-align: center;
  padding: 14px;
  margin: 0;
  font-size: 2.1em;
}

/* Panel content area */
section.panel > ul > li > ul {
  background-color: #f0f0f0;
  flex: 1;
  padding: 14px;
  margin: 0;
  font-size: 0.95em;
  font-weight: normal;
  line-height: 1.6;
  list-style: none;
  /* display: block !important;*/
  transition: background-color 0.3s ease;
  border-bottom-left-radius: 12px;
  border-bottom-right-radius: 12px;
  text-align: center;
}

/* Optional hover shade for body */
section.panel > ul > li:hover > ul {
  background-color: #eaeaea;
}

/* Sub-items flattened */
section.panel > ul > li > ul li {
  list-style: none;
  padding-left: 0;
  margin: 6px 0;
  display: block;
}

/* Color themes */
section.panel li:nth-child(1) > p:first-child { background-color: #2c3e50; }
section.panel li:nth-child(2) > p:first-child { background-color: #8e44ad; }
section.panel li:nth-child(3) > p:first-child { background-color: #16a085; }
section.panel li:nth-child(4) > p:first-child { background-color: #d35400; }

section.panel li:nth-child(1) > ul { background-color: #ecf0f1; border:2px solid #2c3e50; }
section.panel li:nth-child(2) > ul { background-color: #f5eef8; border:2px solid #8e44ad;}
section.panel li:nth-child(3) > ul { background-color: #e8f8f5; border:2px solid #16a085; }
section.panel li:nth-child(4) > ul { background-color: #fbeee6; border:2px solid #d35400; }

/* Responsive stacking */
@media (max-width: 768px) {
  section.panel ul {
    flex-direction: column;
  }
}

